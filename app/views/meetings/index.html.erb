<div ng-app="app">
<div ng-controller="meetingsCTRL" ng-init="getData()">

  <h1><a href="/meetings">All meetings</a></h1>
  <div>Filter by name: <input ng-model="nameFilter" list="names"></div>
  <datalist id="names">
    <option ng-repeat="meeting in meetings">
      {{ meeting.name}}
    </option>
  </datalist>

  <button ng-click="sortByAttribute(startTime)">Order by date</button>

  <div ng-repeat="meeting in meetings | filter: { name: nameFilter } | orderBy: sortAttribute:descendingBoolean | filter: { tags: { name: tagClicked } }">
    <h3><a href="/meetings/{{ meeting.id }}">{{ meeting.name }}</a></h3>
    <p> Tags:
      <div ng-repeat="tag in meeting.tags">
        <p ng-click="clickTag(tag.name)">{{ tag.name }}</a>  
      </div>
    </p>
    <p>Address: {{ meeting.address }}</p>
    <p>Start time: {{ meeting.startTime }}</p>
    <p>End time: {{ meeting.endTime }}</p>
  </div>


</div>
  {{ 1 + 1 }}
</div>